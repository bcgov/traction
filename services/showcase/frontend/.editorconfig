root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.html]
indent_style = space
indent_size = 2

[*.{css,js,json,jsx,scss,ts,tsx,vue}]
indent_style = space
indent_size = 2

[.{babelrc,eslintrc}]
indent_style = space
indent_size = 2

[Jenkinsfile*]
indent_style = space
indent_size = 2
